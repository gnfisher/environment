{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg-main": "#000000",
    "fg-main": "#ffffff",
    "bg-dim": "#1e1e1e",
    "fg-dim": "#989898",
    "bg-alt": "#303030",
    "fg-alt": "#a8a8a8",
    "bg-active": "#7030af",
    "bg-inactive": "#454045",
    "border": "#505050",
    "border-subtle": "#303030",
    "red": "#ff5f59",
    "red-warmer": "#ff6b55",
    "red-cooler": "#ff7f9f",
    "red-faint": "#ff9580",
    "red-intense": "#ff5f5f",
    "red-bg": "#5d3c5f",
    "green": "#44bc44",
    "green-warmer": "#70b900",
    "green-cooler": "#00c06f",
    "green-faint": "#78bf78",
    "green-intense": "#44df44",
    "green-bg": "#2a4045",
    "yellow": "#d0bc00",
    "yellow-warmer": "#fec43f",
    "yellow-cooler": "#dfaf7a",
    "yellow-faint": "#d2b580",
    "yellow-intense": "#efef00",
    "yellow-bg": "#45402a",
    "blue": "#2fafff",
    "blue-warmer": "#79a8ff",
    "blue-cooler": "#00bcff",
    "blue-faint": "#82b0ec",
    "blue-intense": "#338fff",
    "blue-bg": "#303050",
    "magenta": "#feacd0",
    "magenta-warmer": "#f78fe7",
    "magenta-cooler": "#b6a0ff",
    "magenta-faint": "#caa6df",
    "magenta-intense": "#ff66ff",
    "cyan": "#00d3d0",
    "cyan-warmer": "#4ae2f0",
    "cyan-cooler": "#6ae4b9",
    "cyan-faint": "#9ac8e0",
    "cyan-intense": "#00eff0"
  },
  "theme": {
    "primary": {
      "dark": "blue",
      "light": "blue"
    },
    "secondary": {
      "dark": "magenta",
      "light": "magenta"
    },
    "accent": {
      "dark": "cyan",
      "light": "cyan"
    },
    "error": {
      "dark": "red",
      "light": "red"
    },
    "warning": {
      "dark": "yellow",
      "light": "yellow"
    },
    "success": {
      "dark": "green",
      "light": "green"
    },
    "info": {
      "dark": "cyan",
      "light": "cyan"
    },
    "text": {
      "dark": "fg-main",
      "light": "fg-main"
    },
    "textMuted": {
      "dark": "fg-dim",
      "light": "fg-dim"
    },
    "background": {
      "dark": "bg-main",
      "light": "bg-main"
    },
    "backgroundPanel": {
      "dark": "bg-dim",
      "light": "bg-dim"
    },
    "backgroundElement": {
      "dark": "bg-alt",
      "light": "bg-alt"
    },
    "border": {
      "dark": "#505050",
      "light": "#505050"
    },
    "borderActive": {
      "dark": "#2fafff",
      "light": "#2fafff"
    },
    "borderSubtle": {
      "dark": "#303030",
      "light": "#303030"
    },
    "diffAdded": {
      "dark": "green",
      "light": "green"
    },
    "diffRemoved": {
      "dark": "red",
      "light": "red"
    },
    "diffContext": {
      "dark": "fg-dim",
      "light": "fg-dim"
    },
    "diffHunkHeader": {
      "dark": "blue",
      "light": "blue"
    },
    "diffHighlightAdded": {
      "dark": "green-intense",
      "light": "green-intense"
    },
    "diffHighlightRemoved": {
      "dark": "red-intense",
      "light": "red-intense"
    },
    "diffAddedBg": {
      "dark": "green-bg",
      "light": "green-bg"
    },
    "diffRemovedBg": {
      "dark": "red-bg",
      "light": "red-bg"
    },
    "diffContextBg": {
      "dark": "bg-dim",
      "light": "bg-dim"
    },
    "diffLineNumber": {
      "dark": "fg-dim",
      "light": "fg-dim"
    },
    "diffAddedLineNumberBg": {
      "dark": "green-bg",
      "light": "green-bg"
    },
    "diffRemovedLineNumberBg": {
      "dark": "red-bg",
      "light": "red-bg"
    },
    "markdownText": {
      "dark": "fg-main",
      "light": "fg-main"
    },
    "markdownHeading": {
      "dark": "blue",
      "light": "blue"
    },
    "markdownLink": {
      "dark": "blue",
      "light": "blue"
    },
    "markdownLinkText": {
      "dark": "cyan",
      "light": "cyan"
    },
    "markdownCode": {
      "dark": "magenta",
      "light": "magenta"
    },
    "markdownBlockQuote": {
      "dark": "fg-dim",
      "light": "fg-dim"
    },
    "markdownEmph": {
      "dark": "yellow",
      "light": "yellow"
    },
    "markdownStrong": {
      "dark": "red-warmer",
      "light": "red-warmer"
    },
    "markdownHorizontalRule": {
      "dark": "#505050",
      "light": "#505050"
    },
    "markdownListItem": {
      "dark": "blue",
      "light": "blue"
    },
    "markdownListEnumeration": {
      "dark": "cyan",
      "light": "cyan"
    },
    "markdownImage": {
      "dark": "blue",
      "light": "blue"
    },
    "markdownImageText": {
      "dark": "cyan",
      "light": "cyan"
    },
    "markdownCodeBlock": {
      "dark": "fg-main",
      "light": "fg-main"
    },
    "syntaxComment": {
      "dark": "fg-dim",
      "light": "fg-dim"
    },
    "syntaxKeyword": {
      "dark": "magenta",
      "light": "magenta"
    },
    "syntaxFunction": {
      "dark": "blue",
      "light": "blue"
    },
    "syntaxVariable": {
      "dark": "cyan-cooler",
      "light": "cyan-cooler"
    },
    "syntaxString": {
      "dark": "blue-cooler",
      "light": "blue-cooler"
    },
    "syntaxNumber": {
      "dark": "cyan",
      "light": "cyan"
    },
    "syntaxType": {
      "dark": "cyan-warmer",
      "light": "cyan-warmer"
    },
    "syntaxOperator": {
      "dark": "fg-alt",
      "light": "fg-alt"
    },
    "syntaxPunctuation": {
      "dark": "fg-alt",
      "light": "fg-alt"
    }
  }
}
